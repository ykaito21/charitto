<%= simple_form_for(@organization) do |f| %>
  <%= f.input :name, class: "form-control" %>
  <%= f.input :description, class: "form-control" %>
  <%= f.input :org_image, class: "form-control" %>
  <%= f.input :org_image_cache, as: :hidden %>
  <%= f.submit "登録", class: "btn btn-orange" %>
<% end %>
